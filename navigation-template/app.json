{
	"expo": {
		"name": "navigation-template",
		"slug": "navigation-template",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon-balloons.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/balloons.png",
			"resizeMode": "cover",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": [
			"**/*"
		],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router"
		],
		"experiments": {
			"tsconfigPaths": true,
			"typedRoutes": true
		}
	}
}